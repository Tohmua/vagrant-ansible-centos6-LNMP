---
# Username
username: vagrant

# You shouldn't need to change this.
mysql_port: 3306

# MySQL Root Username
mysql_root_db_user: root

# MySQL Root Pass
mysql_root_db_pass: root

# NginX Server Root
nginx_server_root: /vagrant/iaptus/html
nginx_try_files: /slim.php$is_args$args

# Node Version
node_version: stable
